class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
      
        int size = nums.size();
        if (size == 0) return 0;
        
        int j = 0;  
        for (int i = 0; i < size; i++){
            if (nums[i] != nums[j]) nums[++j] = nums[i];
        }
        
        //返回数组的前j+1个
        return j+1;
    }
};
